<Bar Breakpoint="Breakpoint.Desktop" NavigationBreakpoint="Breakpoint.Tablet" ThemeContrast="ThemeContrast.Dark"
     Mode="BarMode.VerticalInline" CollapseMode="BarCollapseMode.Small">
    <BarToggler />
    <BarBrand>
        <BarItem>
            <BarLink To="">
                <BarIcon IconName="IconName.Home" />
                  Strona główna
            </BarLink>
        </BarItem>
    </BarBrand>
    <BarMenu>
        <BarStart>
            <BarItem>
                <BarLink To="/car">
                    <BarIcon IconName="IconName.ShuttleVan" />
                    Zarządzanie samochodami
                </BarLink>
            </BarItem>
        </BarStart>
        <BarEnd>
            <BarItem>
                <BarLink To="/user">
                    <BarIcon IconName="IconName.User" />
                    Konto
                </BarLink>
            </BarItem>
            <BarItem>
                <BarLink To="/login">
                    <BarIcon IconName="IconName.Unlock" />
                    Wyloguj
                </BarLink>
            </BarItem>
        </BarEnd>
    </BarMenu>
</Bar>
